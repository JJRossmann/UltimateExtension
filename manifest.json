{
    "name": "One extension to rule them all",
    "description": "We got this!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "identity.email",
        "identity",
        "history",
        "desktopCapture",
        "tabs"
    ]
}